<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SeasonVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  ComponentClass="mnj.ont.model.lov.views.SeasonVOImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|Prog|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.views.SeasonVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="buyerNameBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mnj.ont.model.lov.views.SeasonVO.buyerNameBind_LABEL"/>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="seasonBind"
    Kind="viewcriteria"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <ViewAccessor
    Name="CiHeadersVO1"
    ViewObjectName="mnj.ont.model.views.CiHeadersVO"
    RowLevelBinds="true"/>
  <SQLQuery>
    <![CDATA[select DISTINCT  qr.CHARACTER2 season, 
                 qr.CHARACTER1 buyer,
                 NVL((SELECT SeasonDetailEO.SEASON_NAME
                       FROM PWC_MFG_BSS_STYLE_L  StyleLineEO,
                            pwc_mfg_bss_buyer_h  pwcmfgbssbuyerh,
                            PWC_MFG_BSS_SEASON_D SeasonDetailEO   
                       WHERE /*StyleLineEO.STYLE_IDENTIFIER = STYLE_NAME
                       AND   */SeasonDetailEO.SEASON_IDENTIFIER = qr.CHARACTER2
                       AND   pwcmfgbssbuyerh.BUYER_ID = (SELECT customer_id
                                                        FROM AR_CUSTOMERS AC
                                                        WHERE AC.CUSTOMER_NAME = qr.CHARACTER1
                                                        AND ROWNUM = 1)
                       --AND   pwcmfgbssbuyerh.ORG_ID = HeaderEO.ORG_ID 
                       AND   StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
                       AND   SeasonDetailEO.LINE_ID = StyleLineEO.LINE_ID 
                       AND   rownum = 1),qr.CHARACTER2) CURRENT_SEASON 
from qa_results_v qr 
WHERE qr.NAME = 'BUYER WISE DETAILS OM'
AND qr.CHARACTER2 IS NOT NULL
/*AND qr.CHARACTER1 = :buyerName*/]]>
  </SQLQuery>
  <ViewAttribute
    Name="Season"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
