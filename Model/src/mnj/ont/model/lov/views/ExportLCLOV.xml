<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ExportLCLOV"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  ComponentClass="mnj.ont.model.lov.views.ExportLCLOVImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll|Prog|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.views.ExportLCLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select  Distinct cont1.EXPORT_LC_NO, 
                (SELECT DISTINCT  tp_header.EXPORT_ORDER_NUMBER 
                 FROM IEDOC_IMP_UD_LINES_CONTRACT contract,
                     IEDOC_IMP_UD_HEADERS headers,
                     IEDOC_IMP_UD_TP_HEADERS tp_header
                WHERE  contract.IMP_UD_HEADER_ID = headers.IMP_UD_HEADER_ID
                AND headers.IMP_UD_TP_HEADER_ID = tp_header.IMP_UD_TP_HEADER_ID
                AND CONT1.EXPORT_LC_NO = contract.CONTRACT_ELC_NO ) UD_NUMBER,
                cont1.CONTRACT_DATE , 
                cont1.COMMISION_PERC,
                cont1.BUYER_ID,
                cont1.BUYER_NAME_NEW BUYER_NAME,
                cont1.SEASON
        from IEDOC_IMP_SALE_CONTR_HEADERS cont1
        WHERE cont1.EXPORT_LC_NO IS NOT NULL]]>
  </SQLQuery>
  <ViewAttribute
    Name="ExportLcNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EXPORT_LC_NO"
    Expression="EXPORT_LC_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="UdNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UD_NUMBER"
    Expression="UD_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
