  <SQLQuery>
    <![CDATA[SELECT CiLinesEO.EXP_CI_LINE_ID, 
       CiLinesEO.EXP_CI_HEADER_ID, 
       CiLinesEO.STYLE,
       NVL((SELECT DECODE(StyleLineEO.TRACK_BASIS,'Style Name',StyleLineEO.STYLE_NAME,'Style Number',StyleLineEO.STYLE_NUMBER)
       FROM PWC_MFG_BSS_STYLE_L StyleLineEO,
            pwc_mfg_bss_buyer_h pwcmfgbssbuyerh       
       WHERE StyleLineEO.STYLE_IDENTIFIER = CiLinesEO.STYLE
       AND   pwcmfgbssbuyerh.BUYER_ID = (SELECT CiHeadersEO.BUYER_ID
                                         FROM IEDOC_EXP_CI_HEADERS CiHeadersEO
                                         WHERE CiHeadersEO.EXP_CI_HEADER_ID = CiLinesEO.EXP_CI_HEADER_ID)
       --AND   pwcmfgbssbuyerh.ORG_ID = HeaderEO.ORG_ID 
       AND   StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
       AND   rownum = 1),CiLinesEO.STYLE) CURRENT_STYLE, 
       CiLinesEO.ORDER_BPO,
       NVL((SELECT OOHA.ATTRIBUTE12
       FROM OE_ORDER_HEADERS_ALL OOHA
       WHERE OOHA.CUST_PO_NUMBER = CiLinesEO.ORDER_BPO
       AND ROWNUM = 1),CiLinesEO.ORDER_BPO) CURRENT_BPO, 
       CiLinesEO.DCPO, 
       CiLinesEO.ARTICLE_NO, 
       CiLinesEO.ITEM_NO, 
       CiLinesEO.ITEM_DESCRIPTION, 
       CiLinesEO.SHIP_QTY, 
       CiLinesEO.UOM, 
       CiLinesEO.UNIT_PRICE, 
       CiLinesEO.GOODS_VALUE, 
       CiLinesEO.TRADE_DISCOUNT_COMMISSION, 
       CiLinesEO.INVOICE_VALUE, 
       CiLinesEO.INCO_TERMS, 
       CiLinesEO.HTS_CODE, 
       CiLinesEO.CARTON_NO, 
       CiLinesEO.CARTON_QUANTITY, 
       CiLinesEO.NET_NET_WEIGHT, 
       CiLinesEO.NET_WEIGHT, 
       CiLinesEO.GROSS_WEIGHT, 
       CiLinesEO.CBM_M3, 
       CiLinesEO.EXCESS_SHORT_QTY, 
       CiLinesEO.PERC_EXCESS_SHORT_QTY, 
       CiLinesEO.SHIPMENT_MODE, 
       CiLinesEO.FREIGHT_CHARGE, 
       CiLinesEO.VERSION_NO, 
       CiLinesEO.VERSION_DATE, 
       CiLinesEO.ORG_ID, 
       CiLinesEO.LAST_UPDATE_LOGIN, 
       CiLinesEO.LAST_UPDATE_DATE, 
       CiLinesEO.LAST_UPDATED_BY, 
       CiLinesEO.CREATION_DATE, 
       CiLinesEO.CREATED_BY, 
       CiLinesEO.ATTRIBUTE1, 
       CiLinesEO.ATTRIBUTE2, 
       CiLinesEO.ATTRIBUTE3, 
       CiLinesEO.ATTRIBUTE4, 
       CiLinesEO.ATTRIBUTE5, 
       CiLinesEO.ATTRIBUTE6, 
       CiLinesEO.ATTRIBUTE7, 
       CiLinesEO.ATTRIBUTE8, 
       CiLinesEO.ATTRIBUTE9, 
       CiLinesEO.ATTRIBUTE10, 
       CiLinesEO.ATTRIBUTE11, 
       CiLinesEO.ATTRIBUTE12, 
       CiLinesEO.ATTRIBUTE13, 
       CiLinesEO.ATTRIBUTE14, 
       CiLinesEO.ATTRIBUTE15, 
       CiLinesEO.ATTRIBUTE16, 
       CiLinesEO.ATTRIBUTE17, 
       CiLinesEO.ATTRIBUTE18, 
       CiLinesEO.ATTRIBUTE19, 
       CiLinesEO.ATTRIBUTE20, 
       CiLinesEO.ATTRIBUTE_NUM1, 
       CiLinesEO.ATTRIBUTE_NUM2, 
       CiLinesEO.ATTRIBUTE_NUM3, 
       CiLinesEO.ATTRIBUTE_NUM4, 
       CiLinesEO.ATTRIBUTE_NUM5, 
       CiLinesEO.ATTRIBUTE_NUM6, 
       CiLinesEO.ATTRIBUTE_NUM7, 
       CiLinesEO.ATTRIBUTE_NUM8, 
       CiLinesEO.ATTRIBUTE_NUM9, 
       CiLinesEO.ATTRIBUTE_NUM10, 
       CiLinesEO.ATTRIBUTE_DATE1, 
       CiLinesEO.ATTRIBUTE_DATE2, 
       CiLinesEO.ATTRIBUTE_DATE3, 
       CiLinesEO.ATTRIBUTE_DATE4, 
       CiLinesEO.ATTRIBUTE_DATE5,
        CiLinesEO.INCO_TERMS_VALUE,
        CiLinesEO.CHARGABLE_GROSS_WEIGHT,
        CiLinesEO.SHIPMENT_TYPE,
        CiLinesEO.TOTAL_CBM,
        CiLinesEO.SHORT_EXCESS_QTY,
        CiLinesEO.SHORT_EXCESS_PER,
        CiLinesEO.PACK_LIST_NO,
        CiLinesEO.COUNTRY,
        CiLinesEO.TRANSPORTATION_MODE,
        CiLinesEO.CURRENCY,
        CiLinesEO.ORDER_ITEM_ID,
        CiLinesEO.ITEM_NAME,
        CiLinesEO.ITEM_CODE,
        CiLinesEO.FINAL_UNIT_PRICE ,
        CiLinesEO.CHILD_STYLE_NO,
        CiLinesEO.HANGER_STYLE,
        CiLinesEO.HANGER_COST,
        CiLinesEO.SIZER_STYLE,
        CiLinesEO.SIZER_COST,
        CiLinesEO.BPO_ID 
FROM IEDOC_EXP_CI_LINES CiLinesEO     
ORDER BY CiLinesEO.PACK_LIST_NO]]>
  </SQLQuery>
