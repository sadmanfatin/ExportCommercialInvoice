  <SQLQuery>
    <![CDATA[SELECT CiHeadersEO.EXP_CI_HEADER_ID, 
       CiHeadersEO.ORG_NAME, 
       CiHeadersEO.BUYER_ID, 
       CiHeadersEO.SEASON,
       NVL((SELECT SeasonDetailEO.SEASON_NAME
       FROM PWC_MFG_BSS_STYLE_L  StyleLineEO,
            pwc_mfg_bss_buyer_h  pwcmfgbssbuyerh,
            PWC_MFG_BSS_SEASON_D SeasonDetailEO   
       WHERE /*StyleLineEO.STYLE_IDENTIFIER = HederEO.STYLE_NAME
       AND   */SeasonDetailEO.SEASON_IDENTIFIER = CiHeadersEO.SEASON
       AND   pwcmfgbssbuyerh.BUYER_ID = CiHeadersEO.BUYER_ID
       --AND   pwcmfgbssbuyerh.ORG_ID = HeaderEO.ORG_ID 
       AND   StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
       AND   SeasonDetailEO.LINE_ID = StyleLineEO.LINE_ID 
       AND   rownum = 1),CiHeadersEO.SEASON) CURRENT_SEASON, 
       CiHeadersEO.INVOICE_NO, 
       CiHeadersEO.INVOICE_DATE, 
       CiHeadersEO.CONTRACT_LC_NUMBER, 
       CiHeadersEO.CONTRACT_LC_DATE, 
       CiHeadersEO.EXPORT_LC_NO, 
       CiHeadersEO.EXPORT_LC_DATE, 
       CiHeadersEO.EXP_NO, 
       CiHeadersEO.EXP_DATE, 
       CiHeadersEO.COMMISSION_PERCENTAGE, 
       CiHeadersEO.VERSION_NO, 
       CiHeadersEO.VERSION_DATE, 
       CiHeadersEO.ORG_ID, 
       CiHeadersEO.LAST_UPDATE_LOGIN, 
       CiHeadersEO.LAST_UPDATE_DATE, 
       CiHeadersEO.LAST_UPDATED_BY, 
       CiHeadersEO.CREATION_DATE, 
       CiHeadersEO.CREATED_BY, 
       CiHeadersEO.ATTRIBUTE1, 
       CiHeadersEO.ATTRIBUTE2, 
       CiHeadersEO.ATTRIBUTE3, 
       CiHeadersEO.ATTRIBUTE4, 
       CiHeadersEO.ATTRIBUTE5, 
       CiHeadersEO.ATTRIBUTE6, 
       CiHeadersEO.ATTRIBUTE7, 
       CiHeadersEO.ATTRIBUTE8, 
       CiHeadersEO.ATTRIBUTE9, 
       CiHeadersEO.ATTRIBUTE10, 
       CiHeadersEO.ATTRIBUTE11, 
       CiHeadersEO.ATTRIBUTE12, 
       CiHeadersEO.ATTRIBUTE13, 
       CiHeadersEO.ATTRIBUTE14, 
       CiHeadersEO.ATTRIBUTE15, 
       CiHeadersEO.ATTRIBUTE16, 
       CiHeadersEO.ATTRIBUTE17, 
       CiHeadersEO.ATTRIBUTE18, 
       CiHeadersEO.ATTRIBUTE19, 
       CiHeadersEO.ATTRIBUTE20, 
       CiHeadersEO.ATTRIBUTE_NUM1, 
       CiHeadersEO.ATTRIBUTE_NUM2, 
       CiHeadersEO.ATTRIBUTE_NUM3, 
       CiHeadersEO.ATTRIBUTE_NUM4, 
       CiHeadersEO.ATTRIBUTE_NUM5, 
       CiHeadersEO.ATTRIBUTE_NUM6, 
       CiHeadersEO.ATTRIBUTE_NUM7, 
       CiHeadersEO.ATTRIBUTE_NUM8, 
       CiHeadersEO.ATTRIBUTE_NUM9, 
       CiHeadersEO.ATTRIBUTE_NUM10, 
       CiHeadersEO.ATTRIBUTE_DATE1, 
       CiHeadersEO.ATTRIBUTE_DATE2, 
       CiHeadersEO.ATTRIBUTE_DATE3, 
       CiHeadersEO.ATTRIBUTE_DATE4, 
       CiHeadersEO.ATTRIBUTE_DATE5,
       (SELECT AC.CUSTOMER_NAME             FROM AR_CUSTOMERS AC           WHERE AC.CUSTOMER_ID = CiHeadersEO.BUYER_ID) BUYER,
       (SELECT INTERNAL_ADDRESS_LINE        FROM HR_ORGANIZATION_UNITS     WHERE ORGANIZATION_ID = CiHeadersEO.ORG_ID) ORG_NAME_LOV,
CiHeadersEO.INVOICE_AMOUNT,
 (SELECT SUM(NVL(CiLinesEO.INVOICE_VALUE,0)) 
        FROM    IEDOC_EXP_CI_LINES CiLinesEO 
        WHERE CiLinesEO.EXP_CI_HEADER_ID = CiHeadersEO.EXP_CI_HEADER_ID) TOTAL_INVOICE_VALUE,
CiHeadersEO.Manufacturer_Name,
CiHeadersEO.TOTAL_CONTRACT_VALUE,
--(SELECT SUM(CiLinesEO.CBM_M3)   
-- FROM IEDOC_EXP_CI_LINES CiLinesEO
-- WHERE CiLinesEO.EXP_CI_HEADER_ID =  CiHeadersEO.EXP_CI_HEADER_ID ) TOTAL_CBM_M3,
CiHeadersEO.TOTAL_CBM_M3,
 (SELECT SUM(CiLinesEO.SHIP_QTY) 
 FROM IEDOC_EXP_CI_LINES CiLinesEO
 WHERE CiLinesEO.EXP_CI_HEADER_ID = CiHeadersEO.EXP_CI_HEADER_ID  ) TOTAL_QUANTITY,
 CiHeadersEO.TOTAL_NET_NET_WT,
 CiHeadersEO.TOTAL_NET_WT,
 CiHeadersEO.TOTAL_GROSS_WEIGHT,
CiHeadersEO.NEGOTIATION_BANK,
CiHeadersEO.BANK_BRANCH,
CiHeadersEO.BANK_ADDRESS,
CiHeadersEO.TOTAL_EXPORTED_VALUE,
NVL(CiHeadersEO.TOTAL_CONTRACT_VALUE,0) - NVL(CiHeadersEO.TOTAL_EXPORTED_VALUE,0) BALANCE,
CiHeadersEO.TOTAL_CARTON_QTY,
CiHeadersEO.Consignee,
CiHeadersEO.CURRENCY,
CiHeadersEO.DOC_NO,
(SELECT  CiLinesEO.DCPO
FROM IEDOC_EXP_CI_LINES CiLinesEO
WHERE CiHeadersEO.EXP_CI_HEADER_ID = CiLinesEO.EXP_CI_HEADER_ID
AND ROWNUM = 1) DCPO,
CiHeadersEO.CONSIGNEE_ADDRESS,
CiHeadersEO.NOTIFY_TO,
CiHeadersEO.NOTIFY_ADDRESS,
CiHeadersEO.SHIP_TO,
CiHeadersEO.SHIP_TO_ADDRESS,
CiHeadersEO.BILL_TO,
CiHeadersEO.CUST_ACCOUNT_ID,
CiHeadersEO.CUST_ACCT_SITE_ID,
CiHeadersEO.BILL_ADDRESS_ID,
CiHeadersEO.BILL_CUSTOMER_ID,
CiHeadersEO.SHIP_ADDRESS_ID,
CiHeadersEO.SHIP_CUSTOMER_ID,
CiHeadersEO.BILL_TO_ADDRESS ,
CiHeadersEO.INVOICE_DATE2 ,

 ( select ta.TRX_NUMBER from RA_CUSTOMER_TRX_ALL ta where ta.ATTRIBUTE13 =  TO_CHAR(CiHeadersEO.DOC_NO)  and  CiHeadersEO.org_id = ta.org_id  and   CiHeadersEO.INVOICE_NO = ta.ATTRIBUTE4 and rownum =1  
     and ta.ATTRIBUTE13 is not null
 
  )  TRX_NUMBER ,
(SELECT listagg ( PACK_LIST_NO, ' , ')within group(order by EXP_CI_HEADER_ID) FROM ( select distinct  ML.PACK_LIST_NO,ML.EXP_CI_HEADER_ID FROM  IEDOC_EXP_CI_LINES ML WHERE ML.EXP_CI_HEADER_ID= CiHeadersEO.EXP_CI_HEADER_ID) ) PACK_LIST_NO 
FROM IEDOC_EXP_CI_HEADERS CiHeadersEO
ORDER BY    CiHeadersEO.DOC_NO desc , 
            CiHeadersEO.VERSION_NO]]>
  </SQLQuery>
