  <SQLQuery>
    <![CDATA[SELECT CiLines2EO.EXP_CI_LINE2_ID, 
       CiLines2EO.EXP_CI_HEADER_ID, 
       CiLines2EO.QUANTITY, 
       CiLines2EO.STYLE,
       NVL((SELECT DECODE(StyleLineEO.TRACK_BASIS,'Style Name',StyleLineEO.STYLE_NAME,'Style Number',StyleLineEO.STYLE_NUMBER)
       FROM PWC_MFG_BSS_STYLE_L StyleLineEO,
            pwc_mfg_bss_buyer_h pwcmfgbssbuyerh       
       WHERE StyleLineEO.STYLE_IDENTIFIER = CiLines2EO.STYLE
       AND   pwcmfgbssbuyerh.BUYER_ID = (SELECT CiHeadersEO.BUYER_ID
                                         FROM IEDOC_EXP_CI_HEADERS CiHeadersEO
                                         WHERE CiHeadersEO.EXP_CI_HEADER_ID = CiLines2EO.EXP_CI_HEADER_ID)
       --AND   pwcmfgbssbuyerh.ORG_ID = HeaderEO.ORG_ID 
       AND   StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
       AND   rownum = 1),CiLines2EO.STYLE) CURRENT_STYLE, 
       CiLines2EO.CONSUMPTION, 
       CiLines2EO.USED_FABRIC, 
       CiLines2EO.ATTRIBUTE4, 
       CiLines2EO.ATTRIBUTE_NUM1, 
       CiLines2EO.ATTRIBUTE_NUM2, 
       CiLines2EO.ATTRIBUTE_NUM3, 
       CiLines2EO.ATTRIBUTE_NUM4, 
       CiLines2EO.ATTRIBUTE_NUM5, 
       CiLines2EO.ATTRIBUTE_NUM6, 
       CiLines2EO.ATTRIBUTE_NUM7, 
       CiLines2EO.ATTRIBUTE_NUM8, 
       CiLines2EO.ATTRIBUTE_NUM9, 
       CiLines2EO.ATTRIBUTE_NUM10, 
       CiLines2EO.ATTRIBUTE_DATE1, 
       CiLines2EO.ATTRIBUTE_DATE2, 
       CiLines2EO.ATTRIBUTE_DATE3, 
       CiLines2EO.ATTRIBUTE_DATE4, 
       CiLines2EO.ATTRIBUTE_DATE5, 
       CiLines2EO.COLOR
FROM IEDOC_EXP_CI_LINES_2 CiLines2EO]]>
  </SQLQuery>
