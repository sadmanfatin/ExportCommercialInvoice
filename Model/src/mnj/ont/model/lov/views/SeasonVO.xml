    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mnj.ont.model.lov.views.SeasonVO.CurrentSeason_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewCriteria
    Name="SeasonVOCriteria"
    ViewObjectName="mnj.ont.model.lov.views.SeasonVO"
    Conjunction="AND">
    <Properties>
      <CustomProperties>
        <Property
          Name="displayOperators"
          Value="InAdvancedMode"/>
        <Property
          Name="autoExecute"
          Value="false"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
    <ViewCriteriaRow
      Name="vcrow8"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="SeasonVOCriteria_vcrow8_Season"
        ViewAttribute="Season"
        Operator="="
        Conjunction="AND"
        Value=":seasonBind"
        IsBindVarValue="true"
        Required="Optional"/>
      <ViewCriteriaItem
        Name="Buyer"
        ViewAttribute="Buyer"
        Operator="="
        Conjunction="AND"
        Value=":buyerNameBind"
        IsBindVarValue="true"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
